{
  "name": "@vibbioinfocore/phylio",
  "version": "0.0.1",
  "description": "Read, write, and manipulate phylogenetic trees",
  "main": "dist/index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build:debug": "spago build && node ./build-debug.js",
    "build": "spago -x javascript.dhall build -u '-o ps_output --codegen corefn' && zephyr -f -e -i ps_output -o output Bio.Phylogeny JsLib && node ./build.js",
    "test": "spago -x test.dhall test && npm run js:test",
    "js:test": "spago -x javascript.dhall build && ava",
    "lint": "tsc --noEmit && eslint 'js/**/*.ts' 'test/js/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vibbits/phylio.git"
  },
  "keywords": [
    "phylogeny",
    "tree",
    "network",
    "Newick",
    "NHX",
    "PhyloXML",
    "NEXUS",
    "Purescript",
    "Typescript"
  ],
  "author": "James Collier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vibbits/phylio/issues"
  },
  "homepage": "https://github.com/vibbits/phylio#readme",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-typescript": "7.18.6",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "ava": "4.3.1",
    "esbuild": "0.15.5",
    "eslint": "8.22.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.1",
    "npm-dts": "1.3.12",
    "purescript": "0.15.4",
    "spago": "0.20.9",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
