{
  "name": "phylio",
  "version": "1.0.0",
  "description": "Read, write, and manipulate phylogenetic trees",
  "main": "dist/index.mjs",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "spago build --purs-args '--codegen corefn' && zephyr -f -e Bio.Phylogeny && esbuild js/main.ts --bundle --minify --platform=browser --format=esm --outfile=dist/index.mjs",
    "test": "spago test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vibbits/phylio.git"
  },
  "keywords": [
    "phylogenetic",
    "tree",
    "newick",
    "NHX",
    "PhyloXML",
    "NEXUS",
    "PureScript",
    "TypeScript"
  ],
  "author": "James Collier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vibbits/phylio/issues"
  },
  "homepage": "https://github.com/vibbits/phylio#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "esbuild": "^0.14.20",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "purescript": "^0.14.5",
    "spago": "^0.20.5",
    "typescript": "^4.5.5"
  }
}
