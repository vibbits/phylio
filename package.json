{
  "name": "@vibbioinfocore/phylio",
  "version": "0.0.1",
  "description": "Read, write, and manipulate phylogenetic trees",
  "main": "dist/index.mjs",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "spago build --purs-args '--codegen corefn' && zephyr -f -e Bio.Phylogeny && esbuild js/main.ts --bundle --minify --platform=browser --format=esm --outfile=dist/index.mjs",
    "build-debug": "spago build && esbuild js/main.ts --bundle --platform=browser --outfile=dist/index-debug.mjs",
    "test": "spago test && npm run test-js",
    "test-js": "jasmine",
    "lint": "tsc --noEmit && eslint 'js/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vibbits/phylio.git"
  },
  "keywords": [
    "phylogeny",
    "tree",
    "network",
    "Newick",
    "NHX",
    "PhyloXML",
    "NEXUS",
    "Purescript",
    "Typescript"
  ],
  "author": "James Collier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vibbits/phylio/issues"
  },
  "homepage": "https://github.com/vibbits/phylio#readme",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.17.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "esbuild": "^0.14.24",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jasmine": "^4.0.2",
    "purescript": "^0.14.7",
    "spago": "^0.20.7",
    "typescript": "^4.6.2"
  }
}
