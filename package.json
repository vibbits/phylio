{
  "name": "@vibbioinfocore/phylio",
  "version": "0.0.1",
  "description": "Read, write, and manipulate phylogenetic trees",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "spago build --purs-args '--codegen corefn' && zephyr -f -e Bio.Phylogeny && node ./build.js",
    "build-debug": "spago build && node ./build-debug.js",
    "test": "spago test && npm run test-js",
    "test-js": "jasmine",
    "lint": "tsc --noEmit && eslint 'js/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vibbits/phylio.git"
  },
  "keywords": [
    "phylogeny",
    "tree",
    "network",
    "Newick",
    "NHX",
    "PhyloXML",
    "NEXUS",
    "Purescript",
    "Typescript"
  ],
  "author": "James Collier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vibbits/phylio/issues"
  },
  "homepage": "https://github.com/vibbits/phylio#readme",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@babel/register": "7.18.6",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "esbuild": "0.14.49",
    "eslint": "8.19.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "jasmine": "4.2.1",
    "npm-dts": "1.3.12",
    "purescript": "0.15.4",
    "spago": "0.20.9",
    "typescript": "4.7.4"
  }
}
